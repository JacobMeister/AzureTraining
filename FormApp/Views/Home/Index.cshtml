@{
    ViewBag.Title = "Home Page";
}

<form action="/Home/Index" method="post" enctype="multipart/form-data">
    <input type="text" name="name" />
    <input type="file" name="photo" />
    <input type="submit" value="Register" />
</form>

@if (ViewBag.RegisteredPeople != null)
{
<table>
    @foreach (var person in ViewBag.RegisteredPeople)
    {
    <tr>
        <td>@person.Name</td>
        <td><img src="@string.Concat("https://azuretrainingformapp.blob.core.windows.net/thumbnails/", person.Thumbnail)" /></td>
    </tr>
    }

</table>
}